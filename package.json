{
  "name": "harmonia-theme",
  "displayName": "Harmonia Theme",
  "description": "A subtle, carefully crafted theme to reduce visual noise while coding.",
  "publisher": "AgusRdz",
  "repository": {
    "type": "git",
    "url": "https://github.com/AgusRdz/harmonia-theme"
  },
  "version": "1.5.5",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "color-theme",
    "dark",
    "light",
    "high contrast",
    "noir",
    "harmonia",
    "oled",
    "paper white",
    "aurora"
  ],
  "icon": "icon.png",
  "license": "MIT",
  "contributes": {
    "themes": [
      {
        "label": "Harmonia Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/harmonia-dark-color-theme.json"
      },
      {
        "label": "Harmonia Light",
        "uiTheme": "vs",
        "path": "./themes/harmonia-light-color-theme.json"
      },
      {
        "label": "Harmonia Noir",
        "uiTheme": "vs-dark",
        "path": "./themes/harmonia-noir-color-theme.json"
      },
      {
        "label": "Harmonia Dark High Contrast",
        "uiTheme": "vs-dark",
        "path": "./themes/harmonia-dark-high-contrast-color-theme.json"
      },
      {
        "label": "Harmonia OLED True Black",
        "uiTheme": "vs-dark",
        "path": "./themes/harmonia-oled-true-black-color-theme.json"
      },
      {
        "label": "Harmonia Paper White",
        "uiTheme": "vs",
        "path": "./themes/harmonia-paper-white-color-theme.json"
      },
      {
        "label": "Harmonia Aurora",
        "uiTheme": "vs-dark",
        "path": "./themes/harmonia-aurora-color-theme.json"
      }
    ]
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "vsce": "^2.15.0"
  }
}