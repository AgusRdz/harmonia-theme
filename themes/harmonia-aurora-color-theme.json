  "semanticTokenColors": {
    "class": "#6EAADB",
    "decorator": "#6FA3D9",
    "enumMember": "#ffe6b3",
    "function": "#6EAADB",
    "interface": "#6EAADB",
    "method": "#6EAADB",
    "namespace": "#6EAADB",
    "operator": "#C8D0E0B3",
    "parameter": "#a0dce0",
    "property": "#8FAED8",
    "property.declaration": "#9BB8E2",
    "regexp": "#98cce3",
    "type": "#6EAADB",
    "variable.readonly": "#ffe6b3"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.block",
        "comment.line",
        "comment.line.double-slash"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8a909b"
      }
    },
    {
      "name": "Comment Punctuation",
      "scope": ["punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5a5f7a"
      }
    },
    {
      "name": "HTML Angle Brackets",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#c5b4d1"
      }
    },
    {
      "name": "JSX/TSX Angle Brackets",
      "scope": [
        "punctuation.definition.tag.jsx",
        "punctuation.definition.tag.tsx"
      ],
      "settings": {
        "foreground": "#c5b4d1"
      }
    },
    {
      "name": "HTML Tag Name",
      "scope": ["entity.name.tag", "support.class.component.html"],
      "settings": {
        "foreground": "#f7a3a8"
      }
    },
    {
      "name": "JSX/TSX Tag Name",
      "scope": ["entity.name.tag.jsx", "entity.name.tag.tsx"],
      "settings": {
        "foreground": "#f7a3a8"
      }
    },
    {
      "name": "Component Tag (JSX/TSX/Vue)",
      "scope": [
        "support.class.component",
        "support.class.component.jsx",
        "support.class.component.tsx",
        "support.class.component.vue"
      ],
      "settings": {
        "foreground": "#6EAADB"
      }
    },
    {
      "name": "HTML/JSX/Vue Attributes",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.vue",
        "meta.directive.vue"
      ],
      "settings": {
        "foreground": "#e8a2c8"
      }
    },
    {
      "name": "Tailwind Utility Classes",
      "scope": [
        "string.quoted.double.html class",
        "string.quoted.single.html class"
      ],
      "settings": {
        "foreground": "#9dd5b8"
      }
    },
    {
      "name": "Attribute Values (HTML/JSX/Vue)",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.quoted.double.jsx",
        "string.quoted.single.jsx",
        "string.quoted.double.tsx",
        "string.quoted.single.tsx",
        "string.quoted.double.vue",
        "string.quoted.single.vue"
      ],
      "settings": {
        "foreground": "#9dd5b8"
      }
    },
    {
      "name": "HTML Text Content",
      "scope": ["text.html.basic", "text.html.derivative"],
      "settings": {
        "foreground": "#e6e6f0"
      }
    },
    {
      "name": "JSX/TSX Text (Children)",
      "scope": [
        "meta.jsx.children",
        "source.jsx meta.jsx.children",
        "source.js meta.jsx.children",
        "source.tsx meta.jsx.children",
        "meta.tag.inline.jsx meta.jsx.children",
        "meta.tag.block.jsx meta.jsx.children",
        "meta.tag.inline.tsx meta.jsx.children",
        "meta.tag.block.tsx meta.jsx.children"
      ],
      "settings": {
        "foreground": "#e6e6f0"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#6FA3D9"
      }
    },
    {
      "name": "Namespaces / Types / Classes",
      "scope": [
        "entity.name.namespace",
        "entity.name.type",
        "meta.use",
        "support.class"
      ],
      "settings": {
        "foreground": "#6EAADB"
      }
    },
    {
      "name": "Strings (Generic)",
      "scope": ["punctuation.definition.string", "string"],
      "settings": {
        "foreground": "#9dd5b8"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "meta.definition.variable.name",
        "variable",
        "support.variable",
        "variable.other.php"
      ],
      "settings": {
        "foreground": "#8cccd2"
      }
    },
    {
      "name": "Parameters",
      "scope": ["meta.function.parameter", "variable.parameter"],
      "settings": {
        "foreground": "#a0dce0"
      }
    },
    {
      "name": "Constants / Language / Character",
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#e6c58a"
      }
    },
    {
      "name": "Keywords & Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#9e8aeb"
      }
    },
    {
      "name": "Italicized Keywords & Operators",
      "scope": [
        "entity.name.method",
        "keyword.control.conditional",
        "keyword.control.flow",
        "keyword.control.loop",
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.default",
        "keyword.control.from",
        "keyword.control.switch",
        "keyword.operator.expression.delete",
        "keyword.operator.expression.in",
        "keyword.operator.expression.of",
        "keyword.operator.new",
        "keyword.operator.sizeof",
        "storage"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown: Heading",
      "scope": ["markup.heading"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#e8e8f0"
      }
    },
    {
      "name": "Markdown: Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f7a3a8"
      }
    },
    {
      "name": "Markdown: Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b0e3c5"
      }
    },
    {
      "name": "Markdown: Inline Code",
      "scope": ["markup.inline.raw.markdown"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a5d8f7"
      }
    },
    {
      "name": "Markdown: Block Quote",
      "scope": ["markup.quote.markdown"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9fdde3"
      }
    },
    {
      "name": "Markdown: Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#9dd5b8"
      }
    },
    {
      "name": "Markdown: Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#f9d79f"
      }
    },
    {
      "name": "Markdown: Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#f7a3a8"
      }
    },
    {
      "name": "Markdown: Separator",
      "scope": ["meta.separator.markdown"],
      "settings": {
        "foreground": "#5D5A7A"
      }
    },
    {
      "name": "Invalid / Illegal",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#f7a3a8"
      }
    },
    {
      "name": "Keyword punctuation (align with Keywords)",
      "scope": [
        "punctuation.definition.keyword",
        "keyword.operator.expression.instanceof",
        "keyword.operator.expression.typeof",
        "keyword.operator.expression.delete"
      ],
      "settings": {
        "foreground": "#9e8aeb"
      }
    },
    {
      "name": "Built-in / primitive types (align with Types/Namespaces)",
      "scope": [
        "support.type.builtin",
        "support.type.primitive",
        "support.type.primitive.ts"
      ],
      "settings": {
        "foreground": "#6EAADB"
      }
    },
    {
      "name": "Language variables (this, super, arguments)",
      "scope": [
        "variable.language",
        "variable.language.this",
        "variable.language.super",
        "variable.language.arguments"
      ],
      "settings": {
        "foreground": "#B0E3C5"
      }
    },
    {
      "name": "Object literal keys (align with HTML/JSX attributes)",
      "scope": [
        "meta.object-literal.key",
        "meta.object-literal.key entity.name",
        "meta.objectliteral.key",
        "meta.object-literal.key string.unquoted"
      ],
      "settings": {
        "foreground": "#e8a2c8"
      }
    },
    {
      "name": "Decorators / Annotations",
      "scope": [
        "meta.decorator",
        "storage.type.annotation",
        "punctuation.decorator"
      ],
      "settings": {
        "foreground": "#6FA3D9"
      }
    },
    {
      "name": "Object / JSON Property Names",
      "scope": [
