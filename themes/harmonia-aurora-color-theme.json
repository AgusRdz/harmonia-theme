{
  "$schema": "vscode://schemas/color-theme",
  "name": "Harmonia Pastel",
  "type": "dark",
  "semanticHighlighting": true,
  "colors": {
    "activityBar.background": "#24232d",
    "activityBar.border": "#00000000",
    "activityBar.foreground": "#e6e6f0",
    "activityBarBadge.background": "#e8a2c8",
    "activityBarBadge.foreground": "#1a1b26",
    "badge.background": "#c39ed8",
    "badge.foreground": "#1a1b26",
    "breadcrumb.background": "#262533",
    "breadcrumb.foreground": "#a8a9c6",
    "breadcrumb.focusForeground": "#ffffff",
    "button.background": "#b7a0ff",
    "button.foreground": "#1a1b26",
    "button.hoverBackground": "#a48be0",
    "dropdown.border": "#00000000",
    "editor.background": "#1d1c28",
    "editor.foreground": "#e8e8f0",
    "editor.lineHighlightBackground": "#2a2939",
    "editor.lineHighlightBorder": "#2e2f40",
    "editor.selectionBackground": "#433d5c",
    "editor.selectionForeground": "#ffffff",
    "editorCursor.foreground": "#e8e8f0",
    "editorBracketMatch.background": "#44445c",
    "editorBracketMatch.border": "#b7a0ff",
    "editorGroup.border": "#2c2c3c",
    "editorGutter.addedBackground": "#b0e3c5",
    "editorGutter.deletedBackground": "#f7a3a8",
    "editorGutter.modifiedBackground": "#a5d8f7",
    "editorLineNumber.foreground": "#575a74",
    "editorWhitespace.foreground": "#37384a",
    "editor.inactiveSelectionBackground": "#433d5cAA",
    "editor.selectionHighlightBackground": "#a48be033",
    "editor.findMatchBackground": "#b7a0ff66",
    "editor.findMatchHighlightBackground": "#b7a0ff33",
    "editor.findRangeHighlightBackground": "#ffffff00",
    "editor.hoverHighlightBackground": "#a5d8f71f",
    "editor.foldBackground": "#2a293933",
    "editorIndentGuide.activeBackground1": "#5d5f78",
    "editorIndentGuide.background1": "#2a2b3a",
    "editorLineNumber.activeForeground": "#e0e0f0",
    "list.deemphasizedForeground": "#a8a9c6",
    "list.focusOutline": "#00000000",
    "list.focusAndSelectionOutline": "#00000000",

    "tab.border": "#2c2c3c",
    "gitDecoration.addedResourceForeground": "#b0e3c5",
    "gitDecoration.modifiedResourceForeground": "#b7a0ff",
    "gitDecoration.deletedResourceForeground": "#f5b2b7",
    "gitDecoration.conflictingResourceForeground": "#f9d79f",
    "gitDecoration.renamedResourceForeground": "#a5d8f7",
    "gitDecoration.ignoredResourceForeground": "#575a74",
    "gitDecoration.untrackedResourceForeground": "#c1eed3",
    "gitDecoration.submoduleResourceForeground": "#f0b4d6",
    "scmGraph.foreground1": "#b7a0ff",
    "scmGraph.foreground2": "#f0b4d6",
    "scmGraph.foreground3": "#a5d8f7",
    "scmGraph.foreground4": "#b0e3c5",
    "scmGraph.foreground5": "#a8a9c6",
    "scmGraph.historyItemBaseRefColor": "#a5d8f7",
    "scmGraph.historyItemHoverAdditionsForeground": "#b9e9cf",
    "scmGraph.historyItemHoverDefaultLabelBackground": "#2d2c40",
    "scmGraph.historyItemHoverDefaultLabelForeground": "#ffffff",
    "scmGraph.historyItemHoverDeletionsForeground": "#f5b2b7",
    "scmGraph.historyItemHoverLabelForeground": "#ffffff",
    "scmGraph.historyItemRefColor": "#b7a0ff",
    "scmGraph.historyItemRemoteRefColor": "#b9e9cf",
    "textLink.foreground": "#a5d8f7",
    "textLink.activeForeground": "#b0e3c5",
    "statusBarItem.remoteBackground": "#00000000",
    "statusBarItem.remoteForeground": "#e6e6f0",
    "focusBorder": "#00000000",
    "terminal.findMatchBackground": "#b7a0ff66",
    "terminal.findMatchHighlightBackground": "#b7a0ff33",
    "terminal.hoverHighlightBackground": "#2a293933",
    "terminal.tab.activeBorder": "#b7a0ff",
    "list.activeSelectionBackground": "#3d3c58",
    "list.activeSelectionForeground": "#ffffff",
    "list.hoverBackground": "#393850",
    "list.inactiveSelectionBackground": "#2d2c40",
    "list.inactiveSelectionForeground": "#e0e0f0",
    "panel.background": "#1d1c28",
    "panel.border": "#2c2c3c",
    "sideBar.background": "#1d1c28",
    "sideBar.foreground": "#e6e6f0",
    "statusBar.background": "#1d1c28",
    "statusBar.foreground": "#e6e6f0",
    "statusBar.debuggingBackground": "#f7a3a8",
    "statusBar.debuggingForeground": "#1a1b26",
    "tab.activeBackground": "#1d1c28",
    "tab.activeForeground": "#ffffff",
    "tab.inactiveBackground": "#24232d",
    "tab.inactiveForeground": "#a8a9c6",
    "terminal.background": "#1d1c28",
    "terminal.foreground": "#e6e6f0",
    "terminal.ansiBlack": "#2e2f3a",
    "terminal.ansiRed": "#f7a3a8",
    "terminal.ansiGreen": "#b0e3c5",
    "terminal.ansiYellow": "#f9d79f",
    "terminal.ansiBlue": "#a5d8f7",
    "terminal.ansiMagenta": "#e8a2c8",
    "terminal.ansiCyan": "#9fdde3",
    "terminal.ansiWhite": "#e8e8f0",
    "terminal.ansiBrightBlack": "#575a74",
    "terminal.ansiBrightRed": "#f5b2b7",
    "terminal.ansiBrightGreen": "#c1eed3",
    "terminal.ansiBrightYellow": "#ffe6b3",
    "terminal.ansiBrightBlue": "#b8e3fb",
    "terminal.ansiBrightMagenta": "#f0b4d6",
    "terminal.ansiBrightCyan": "#b4e9ed",
    "terminal.ansiBrightWhite": "#ffffff"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.block",
        "comment.line",
        "comment.line.double-slash"
      ],
      "settings": { "foreground": "#9ca3af", "fontStyle": "italic" }
    },
    {
      "name": "Comment Punctuation",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#5a5f7a", "fontStyle": "italic" }
    },
    {
      "name": "HTML Angle Brackets",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.html"
      ],
      "settings": { "foreground": "#a8a9c6" }
    },
    {
      "name": "JSX/TSX Angle Brackets",
      "scope": [
        "punctuation.definition.tag.jsx",
        "punctuation.definition.tag.tsx"
      ],
      "settings": { "foreground": "#a8a9c6" }
    },
    {
      "name": "HTML Tag Name",
      "scope": ["entity.name.tag", "support.class.component.html"],
      "settings": { "foreground": "#f7a3a8" }
    },
    {
      "name": "JSX/TSX Tag Name",
      "scope": ["entity.name.tag.jsx", "entity.name.tag.tsx"],
      "settings": { "foreground": "#f7a3a8" }
    },
    {
      "name": "Component Tag (JSX/TSX/Vue)",
      "scope": [
        "support.class.component",
        "support.class.component.jsx",
        "support.class.component.tsx",
        "support.class.component.vue"
      ],
      "settings": { "foreground": "#b7a0ff" }
    },
    {
      "name": "HTML/JSX/Vue Attributes",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.vue",
        "meta.directive.vue"
      ],
      "settings": { "foreground": "#e8a2c8" }
    },
    {
      "name": "Attribute Values (HTML/JSX/Vue)",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.quoted.double.jsx",
        "string.quoted.single.jsx",
        "string.quoted.double.tsx",
        "string.quoted.single.tsx",
        "string.quoted.double.vue",
        "string.quoted.single.vue"
      ],
      "settings": { "foreground": "#b0e3c5" }
    },
    {
      "name": "Tailwind Utility Classes",
      "scope": [
        "string.quoted.double.html class",
        "string.quoted.single.html class"
      ],
      "settings": { "foreground": "#b0e3c5" }
    },
    {
      "name": "HTML Text Content",
      "scope": ["text.html.basic", "text.html.derivative"],
      "settings": { "foreground": "#e8e8f0" }
    },
    {
      "name": "JSX/TSX Text (Children)",
      "scope": [
        "meta.jsx.children",
        "source.jsx meta.jsx.children",
        "source.js meta.jsx.children",
        "source.tsx meta.jsx.children",
        "meta.tag.inline.jsx meta.jsx.children",
        "meta.tag.block.jsx meta.jsx.children",
        "meta.tag.inline.tsx meta.jsx.children",
        "meta.tag.block.tsx meta.jsx.children"
      ],
      "settings": { "foreground": "#e8e8f0" }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#a5d8f7" }
    },
    {
      "name": "Namespaces / Types / Classes",
      "scope": [
        "entity.name.namespace",
        "entity.name.type",
        "meta.use",
        "support.class"
      ],
      "settings": { "foreground": "#FFE6B3" }
    },
    {
      "name": "Strings (Generic)",
      "scope": ["punctuation.definition.string", "string"],
      "settings": { "foreground": "#b0e3c5" }
    },
    {
      "name": "Variables",
      "scope": ["meta.definition.variable.name", "variable"],
      "settings": { "foreground": "#9fdde3" }
    },
    {
      "name": "Parameters",
      "scope": ["meta.function.parameter", "variable.parameter"],
      "settings": { "foreground": "#b4e9ed" }
    },
    {
      "name": "Constants / Language / Character",
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": { "foreground": "#f9d79f" }
    },
    {
      "name": "Keywords & Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#b7a0ff" }
    },
    {
      "name": "Italicized Keywords & Operators",
      "scope": [
        "entity.name.method",
        "keyword.control.conditional",
        "keyword.control.flow",
        "keyword.control.loop",
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.default",
        "keyword.control.from",
        "keyword.control.switch",
        "keyword.operator.expression.delete",
        "keyword.operator.expression.in",
        "keyword.operator.expression.of",
        "keyword.operator.new",
        "keyword.operator.sizeof",
        "storage"
      ],
      "settings": { "fontStyle": "italic" }
    },
    {
      "name": "Markdown: Heading",
      "scope": ["markup.heading"],
      "settings": { "foreground": "#e8e8f0", "fontStyle": "bold" }
    },
    {
      "name": "Markdown: Bold",
      "scope": ["markup.bold"],
      "settings": { "foreground": "#f7a3a8", "fontStyle": "bold" }
    },
    {
      "name": "Markdown: Italic",
      "scope": ["markup.italic"],
      "settings": { "foreground": "#b0e3c5", "fontStyle": "italic" }
    },
    {
      "name": "Markdown: Inline Code",
      "scope": ["markup.inline.raw.markdown"],
      "settings": { "foreground": "#a5d8f7", "fontStyle": "italic" }
    },
    {
      "name": "Markdown: Block Quote",
      "scope": ["markup.quote.markdown"],
      "settings": { "foreground": "#9fdde3", "fontStyle": "italic" }
    },
    {
      "name": "Markdown: Inserted",
      "scope": ["markup.inserted"],
      "settings": { "foreground": "#b0e3c5" }
    },
    {
      "name": "Markdown: Changed",
      "scope": ["markup.changed"],
      "settings": { "foreground": "#f9d79f" }
    },
    {
      "name": "Markdown: Deleted",
      "scope": ["markup.deleted"],
      "settings": { "foreground": "#f7a3a8" }
    },
    {
      "name": "Markdown: Separator",
      "scope": ["meta.separator.markdown"],
      "settings": { "foreground": "#575a74" }
    },
    {
      "name": "Invalid / Illegal",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#f7a3a8" }
    }
  ]
}
