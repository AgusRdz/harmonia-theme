  "name": "Harmonia Aurora",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "class": "#6EAADB",
    "decorator": "#88C3FF",
    "enumMember": "#E0A96D",
    "function": "#6EAADB",
    "interface": "#6EAADB",
    "method": "#6EAADB",
    "namespace": "#6EAADB",
    "operator": "#C8D0E0B3",
    "parameter": "#7EBF87",
    "property": "#8FAED8",
    "property.declaration": "#9BB8E2",
    "regexp": "#4AB5B4",
    "type": "#6EAADB",
    "variable.readonly": "#E0A96D"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.block",
        "comment.line",
        "comment.line.double-slash"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9ca3af"
      }
    },
    {
      "name": "Comment Punctuation",
      "scope": ["punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5a5f7a"
      }
    },
    {
      "name": "HTML Angle Brackets",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#a8a9c6"
      }
    },
    {
      "name": "JSX/TSX Angle Brackets",
      "scope": [
        "punctuation.definition.tag.jsx",
        "punctuation.definition.tag.tsx"
      ],
      "settings": {
        "foreground": "#a8a9c6"
      }
    },
    {
      "name": "HTML Tag Name",
      "scope": ["entity.name.tag", "support.class.component.html"],
      "settings": {
        "foreground": "#f7a3a8"
      }
    },
    {
      "name": "JSX/TSX Tag Name",
      "scope": ["entity.name.tag.jsx", "entity.name.tag.tsx"],
      "settings": {
        "foreground": "#f7a3a8"
      }
    },
    {
      "name": "Component Tag (JSX/TSX/Vue)",
      "scope": [
        "support.class.component",
        "support.class.component.jsx",
        "support.class.component.tsx",
        "support.class.component.vue"
      ],
      "settings": {
        "foreground": "#b7a0ff"
      }
    },
    {
      "name": "HTML/JSX/Vue Attributes",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.vue",
        "meta.directive.vue"
      ],
      "settings": {
        "foreground": "#e8a2c8"
      }
    },
    {
      "name": "Attribute Values (HTML/JSX/Vue)",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.quoted.double.jsx",
        "string.quoted.single.jsx",
        "string.quoted.double.tsx",
        "string.quoted.single.tsx",
        "string.quoted.double.vue",
        "string.quoted.single.vue"
      ],
      "settings": {
        "foreground": "#b0e3c5"
      }
    },
    {
      "name": "Tailwind Utility Classes",
      "scope": [
        "string.quoted.double.html class",
        "string.quoted.single.html class"
      ],
      "settings": {
        "foreground": "#b0e3c5"
      }
    },
    {
      "name": "HTML Text Content",
      "scope": ["text.html.basic", "text.html.derivative"],
      "settings": {
        "foreground": "#e8e8f0"
      }
    },
    {
      "name": "JSX/TSX Text (Children)",
      "scope": [
        "meta.jsx.children",
        "source.jsx meta.jsx.children",
        "source.js meta.jsx.children",
        "source.tsx meta.jsx.children",
        "meta.tag.inline.jsx meta.jsx.children",
        "meta.tag.block.jsx meta.jsx.children",
        "meta.tag.inline.tsx meta.jsx.children",
        "meta.tag.block.tsx meta.jsx.children"
      ],
      "settings": {
        "foreground": "#e8e8f0"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#a5d8f7"
      }
    },
    {
      "name": "Namespaces / Types / Classes",
      "scope": [
        "entity.name.namespace",
        "entity.name.type",
        "meta.use",
        "support.class"
      ],
      "settings": {
        "foreground": "#FFE6B3"
      }
    },
    {
      "name": "Strings (Generic)",
      "scope": ["punctuation.definition.string", "string"],
      "settings": {
        "foreground": "#b0e3c5"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "meta.definition.variable.name",
        "variable",
        "support.variable",
        "variable.other.php"
      ],
      "settings": {
        "foreground": "#9fdde3"
      }
    },
    {
      "name": "Parameters",
      "scope": ["meta.function.parameter", "variable.parameter"],
      "settings": {
        "foreground": "#b4e9ed"
      }
    },
    {
      "name": "Constants / Language / Character",
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#f9d79f"
      }
    },
    {
      "name": "Keywords & Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#b7a0ff"
      }
    },
    {
      "name": "Italicized Keywords & Operators",
      "scope": [
        "entity.name.method",
        "keyword.control.conditional",
        "keyword.control.flow",
        "keyword.control.loop",
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.default",
        "keyword.control.from",
        "keyword.control.switch",
        "keyword.operator.expression.delete",
        "keyword.operator.expression.in",
        "keyword.operator.expression.of",
        "keyword.operator.new",
        "keyword.operator.sizeof",
        "storage"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown: Heading",
      "scope": ["markup.heading"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#e8e8f0"
      }
    },
    {
      "name": "Markdown: Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#f7a3a8"
      }
    },
    {
      "name": "Markdown: Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#b0e3c5"
      }
    },
    {
      "name": "Markdown: Inline Code",
      "scope": ["markup.inline.raw.markdown"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#a5d8f7"
      }
    },
    {
      "name": "Markdown: Block Quote",
      "scope": ["markup.quote.markdown"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9fdde3"
      }
    },
    {
      "name": "Markdown: Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#b0e3c5"
      }
    },
    {
      "name": "Markdown: Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#f9d79f"
      }
    },
    {
      "name": "Markdown: Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#f7a3a8"
      }
    },
    {
      "name": "Markdown: Separator",
      "scope": ["meta.separator.markdown"],
      "settings": {
        "foreground": "#575a74"
      }
    },
    {
      "name": "Invalid / Illegal",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#f7a3a8"
      }
    },
    {
      "scope": ["punctuation.definition.keyword"],
      "settings": {
        "foreground": "#E8A2C8"
      }
    },
    {
      "scope": ["support.type.builtin"],
      "settings": {
        "foreground": "#A5D8F7"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#B0E3C5"
      }
    },
    {
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#A5D8F7"
      }
    },
    {
      "scope": ["meta.decorator"],
      "settings": {
        "foreground": "#A5D8F7D0"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#CCD2EA"
      }
    },
    {
      "scope": ["punctuation.accessor"],
      "settings": {
        "foreground": "#DDD9E699"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#DDD9E6B3"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#A5D8F7CC"
      }
    },
    {
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#B0E3C5CC"
      }
    },
    {
      "name": "PHP use keyword",
      "scope": [
        "keyword.other.use",
        "keyword.other.namespace.use",
        "keyword.other.use.php"
      ],
      "settings": {
        "foreground": "#C9A7F5"
      }
    },
    {
      "name": "Class and implements keywords",
      "scope": [
        "keyword.declaration.class",
        "storage.type.class",
        "keyword.declaration.interface",
        "keyword.other.implements",
        "storage.modifier.implements"
      ],
      "settings": {
        "foreground": "#C9A7F5"
      }
    },
    {
      "name": "Type and namespace identifiers",
      "scope": [
        "entity.name.type",
        "entity.name.type.class",
        "entity.name.type.interface",
        "entity.other.inherited-class",
        "support.class",
        "support.type",
        "entity.name.namespace",
        "support.other.namespace"
      ],
      "settings": {
        "foreground": "#B6C3FF"
      }
    },
    {
      "name": "Namespace and punctuation separators",
      "scope": [
        "punctuation.separator.namespace",
        "punctuation.accessor",
        "punctuation.separator",
        "punctuation.separator.inheritance"
      ],
      "settings": {
        "foreground": "#C8CAD6"
      }
    },
    {
      "name": "this/self",
      "scope": ["variable.language.this", "variable.language.self"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8CC6EFCC"
      }
    },
    {
      "name": "PHP variable sigil matches variable color",
      "scope": [
        "punctuation.definition.variable.php",
        "variable.other.php punctuation.definition.variable"
      ],
      "settings": { "foreground": "#B6BDCFB3" }
    }
  ],
  "type": "dark"
}
