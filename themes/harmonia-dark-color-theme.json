{
  "$schema": "vscode://schemas/color-theme",
  "name": "Harmonia Dark",
  "semanticHighlighting": true,
  "type": "dark",
  "colors": {
    "activityBar.background": "#1a1b26",
    "activityBar.border": "#00000000",
    "activityBar.foreground": "#d0d0e0",
    "activityBarBadge.background": "#c574dd",
    "activityBarBadge.foreground": "#1a1b26",
    "badge.background": "#8656a8",
    "badge.foreground": "#e6e6f0",
    "breadcrumb.background": "#1e1f2a",
    "breadcrumb.focusForeground": "#b8bdf0",
    "breadcrumb.foreground": "#777a96",
    "button.background": "#9b6ebf",
    "button.border": "#00000000",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#8e5eb2",
    "diffEditor.border": "#5a5f7a",
    "diffEditor.insertedLineBackground": "#a3d9a520",
    "diffEditor.insertedTextBackground": "#1a512a88",
    "diffEditor.removedLineBackground": "#ec727920",
    "diffEditor.removedTextBackground": "#6e1d1d88",
    "dropdown.border": "#00000000",
    "editor.background": "#1a1b26",
    "editor.focusedStackFrameHighlightBackground": "#00000000",
    "editor.foreground": "#d0d0e0",
    "editor.inactiveSelectionBackground": "#2c2e40AA",
    "editor.lineHighlightBackground": "#2a2b38",
    "editor.lineHighlightBorder": "#2e2f3a",
    "editor.selectionBackground": "#37527D",
    "editor.selectionForeground": "#ffffff",
    "editor.selectionHighlightBackground": "#37527D66",
    "editorBracketMatch.background": "#444769",
    "editorBracketMatch.border": "#5a5f7a",
    "editorCursor.foreground": "#d0d0e0",
    "editorGhostText.foreground": "#C5CBE3",
    "editorGroup.border": "#2c2e40",
    "editorGroupHeader.noTabsBackground": "#21222e",
    "editorGroupHeader.tabsBackground": "#21222e",
    "editorGutter.addedBackground": "#a3d9a5",
    "editorGutter.background": "#1a1b26",
    "editorGutter.commentRangeForeground": "#8bd4e9",
    "editorGutter.deletedBackground": "#ec7279",
    "editorGutter.modifiedBackground": "#6ebddb",
    "editorIndentGuide.activeBackground1": "#5a5f7a",
    "editorIndentGuide.background1": "#2c2f40",
    "editorLineNumber.foreground": "#4c4f68",
    "editorWhitespace.foreground": "#2e3140",
    "focusBorder": "#00000000",
    "gitDecoration.addedResourceForeground": "#a3d9a5",
    "gitDecoration.conflictingResourceForeground": "#e0a96d",
    "gitDecoration.deletedResourceForeground": "#ec7279",
    "gitDecoration.ignoredResourceForeground": "#5a5f7a",
    "gitDecoration.modifiedResourceForeground": "#c574dd",
    "gitDecoration.renamedResourceForeground": "#6ebddb",
    "gitDecoration.submoduleResourceForeground": "#94E2D5",
    "gitDecoration.untrackedResourceForeground": "#a3d9a5",
    "input.background": "#1a1b26",
    "input.border": "#2E2E40",
    "input.foreground": "#E6E6F2",
    "input.placeholderForeground": "#A9A9C2",
    "list.activeSelectionBackground": "#353753",
    "list.activeSelectionForeground": "#e0e0f0",
    "list.deemphasizedForeground": "#888ca0",
    "list.focusAndSelectionOutline": "#00000000",
    "list.focusOutline": "#00000000",
    "list.hoverBackground": "#393b58",
    "list.inactiveSelectionBackground": "#2c2e40",
    "list.inactiveSelectionForeground": "#d0d0e0",
    "panel.background": "#1a1b26",
    "panel.border": "#2c2e40",
    "quickInput.background": "#1a1b26",
    "quickInput.foreground": "#E6E6F2",
    "quickInputList.focusBackground": "#222238",
    "quickInputList.focusForeground": "#FFFFFF",
    "quickInputList.focusIconForeground": "#AFC6FF",
    "quickInputTitle.background": "#0D0D14",
    "scmGraph.foreground1": "#bb7dd9",
    "scmGraph.foreground2": "#ef874b",
    "scmGraph.foreground3": "#8bd4e9",
    "scmGraph.foreground4": "#a3d9a5",
    "scmGraph.foreground5": "#888ca0",
    "scmGraph.historyItemBaseRefColor": "#6ebddb",
    "scmGraph.historyItemHoverAdditionsForeground": "#83b987",
    "scmGraph.historyItemHoverDefaultLabelBackground": "#2c2e40",
    "scmGraph.historyItemHoverDefaultLabelForeground": "#e6e6f0",
    "scmGraph.historyItemHoverDeletionsForeground": "#de7d84",
    "scmGraph.historyItemHoverLabelForeground": "#1a1b26",
    "scmGraph.historyItemRefColor": "#AFC6FF",
    "scmGraph.historyItemRemoteRefColor": "#83b987",
    "scrollbar.shadow": "#00000050",
    "sideBar.background": "#1a1b26",
    "sideBar.border": "#00000000",
    "sideBar.foreground": "#d0d0e0",
    "sideBarSectionHeader.background": "#1a1b26",
    "sideBarSectionHeader.border": "#1a1b26",
    "sideBarSectionHeader.foreground": "#D6D7EA",
    "statusBar.background": "#1a1b26",
    "statusBar.debuggingBackground": "#c574dd",
    "statusBar.debuggingForeground": "#1a1b26",
    "statusBar.foreground": "#d0d0e0",
    "statusBar.noFolderBackground": "#1a1b26",
    "statusBar.noFolderForeground": "#d0d0e0",
    "statusBarItem.remoteBackground": "#1a1b26",
    "statusBarItem.remoteForeground": "#e6e6f0",
    "tab.activeBackground": "#1a1b26",
    "tab.activeForeground": "#ffffff",
    "tab.border": "#2c2e40",
    "tab.inactiveBackground": "#21222e",
    "tab.inactiveForeground": "#777a96",
    "terminal.ansiBlack": "#2e2f3a",
    "terminal.ansiBlue": "#6ebddb",
    "terminal.ansiBrightBlack": "#5a5f7a",
    "terminal.ansiBrightBlue": "#8bd4e9",
    "terminal.ansiBrightCyan": "#a3dee0",
    "terminal.ansiBrightGreen": "#b6e3b4",
    "terminal.ansiBrightMagenta": "#d995ed",
    "terminal.ansiBrightRed": "#ec8a8f",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.ansiBrightYellow": "#f0c685",
    "terminal.ansiCyan": "#88c6c3",
    "terminal.ansiGreen": "#a3d9a5",
    "terminal.ansiMagenta": "#c574dd",
    "terminal.ansiRed": "#ec7279",
    "terminal.ansiWhite": "#d0d0e0",
    "terminal.ansiYellow": "#e0a96d",
    "terminal.background": "#1a1b26",
    "terminal.border": "#2c2e40",
    "terminal.findMatchBackground": "#50467980",
    "terminal.findMatchHighlightBackground": "#50467940",
    "terminal.foreground": "#d0d0e0",
    "terminal.hoverHighlightBackground": "#2c2e40",
    "terminal.selectionBackground": "#3b4d6a",
    "terminal.tab.activeBorder": "#c574dd",
    "terminalCursor.foreground": "#d0d0e0",
    "textLink.activeForeground": "#83D3B2",
    "textLink.foreground": "#7BAEF5",
    "titleBar.activeBackground": "#1a1b26"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.block",
        "comment.line",
        "comment.line.double-slash"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9ca3af"
      }
    },
    {
      "name": "Comment Punctuation",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5a5f7a"
      }
    },
    {
      "name": "HTML Angle Brackets",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#b0b0b0"
      }
    },
    {
      "name": "JSX/TSX Angle Brackets",
      "scope": [
        "punctuation.definition.tag.jsx",
        "punctuation.definition.tag.tsx"
      ],
      "settings": {
        "foreground": "#B0B0B0"
      }
    },
    {
      "name": "HTML Tag Name",
      "scope": ["entity.name.tag", "support.class.component.html"],
      "settings": {
        "foreground": "#dcbfa8"
      }
    },
    {
      "name": "JSX/TSX Tag Name",
      "scope": ["entity.name.tag.jsx", "entity.name.tag.tsx"],
      "settings": {
        "foreground": "#DCBFA8"
      }
    },
    {
      "name": "Component Tag (JSX/TSX/Vue)",
      "scope": [
        "support.class.component",
        "support.class.component.jsx",
        "support.class.component.tsx",
        "support.class.component.vue"
      ],
      "settings": {
        "foreground": "#B8BDF0"
      }
    },
    {
      "name": "HTML/JSX/Vue Attributes",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.vue",
        "meta.directive.vue"
      ],
      "settings": {
        "foreground": "#8CA9D5"
      }
    },
    {
      "name": "Tailwind Utility Classes",
      "scope": [
        "string.quoted.double.html class",
        "string.quoted.single.html class"
      ],
      "settings": {
        "foreground": "#a3d9a5"
      }
    },
    {
      "name": "Attribute Values (HTML/JSX/Vue)",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.quoted.double.jsx",
        "string.quoted.single.jsx",
        "string.quoted.double.tsx",
        "string.quoted.single.tsx",
        "string.quoted.double.vue",
        "string.quoted.single.vue"
      ],
      "settings": {
        "foreground": "#A3D9A5"
      }
    },
    {
      "name": "HTML Text Content",
      "scope": ["text.html.basic", "text.html.derivative"],
      "settings": {
        "foreground": "#D0D0E0"
      }
    },
    {
      "name": "JSX/TSX Text (Children)",
      "scope": [
        "meta.jsx.children",
        "source.jsx meta.jsx.children",
        "source.js meta.jsx.children",
        "source.tsx meta.jsx.children",
        "meta.tag.inline.jsx meta.jsx.children",
        "meta.tag.block.jsx meta.jsx.children",
        "meta.tag.inline.tsx meta.jsx.children",
        "meta.tag.block.tsx meta.jsx.children"
      ],
      "settings": {
        "foreground": "#D0D0E0"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#9CB8FF"
      }
    },
    {
      "name": "Namespaces / Types / Classes",
      "scope": [
        "entity.name.namespace",
        "entity.name.type",
        "meta.use",
        "support.class"
      ],
      "settings": {
        "foreground": "#b8bdf0"
      }
    },
    {
      "name": "Strings (Generic)",
      "scope": ["punctuation.definition.string", "string"],
      "settings": {
        "foreground": "#a3d9a5"
      }
    },
    {
      "name": "Variables",
      "scope": ["meta.definition.variable.name", "variable"],
      "settings": {
        "foreground": "#88c6c3"
      }
    },
    {
      "name": "Parameters",
      "scope": ["meta.function.parameter", "variable.parameter"],
      "settings": {
        "foreground": "#94E2D5"
      }
    },
    {
      "name": "Constants / Language / Character",
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#e0a96d"
      }
    },
    {
      "name": "Keywords & Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#c574dd"
      }
    },
    {
      "name": "Italicized Keywords & Operators",
      "scope": [
        "entity.name.method",
        "keyword.control.conditional",
        "keyword.control.flow",
        "keyword.control.loop",
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.default",
        "keyword.control.from",
        "keyword.control.switch",
        "keyword.operator.expression.delete",
        "keyword.operator.expression.in",
        "keyword.operator.expression.of",
        "keyword.operator.new",
        "keyword.operator.sizeof",
        "storage"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown: Heading",
      "scope": ["markup.heading"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#D9E0EE"
      }
    },
    {
      "name": "Markdown: Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#CBA6F7"
      }
    },
    {
      "name": "Markdown: Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#BAC2DE"
      }
    },
    {
      "name": "Markdown: Inline Code",
      "scope": ["markup.inline.raw.markdown"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#A6E3A1"
      }
    },
    {
      "name": "Markdown: Block Quote",
      "scope": ["markup.quote.markdown"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#94E2D5"
      }
    },
    {
      "name": "Markdown: Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#a3d9a5"
      }
    },
    {
      "name": "Markdown: Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#e0a96d"
      }
    },
    {
      "name": "Markdown: Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#ec7279"
      }
    },
    {
      "name": "Markdown: Separator",
      "scope": ["meta.separator.markdown"],
      "settings": {
        "foreground": "#5a5f7a"
      }
    },
    {
      "name": "Invalid / Illegal",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ec7279"
      }
    }
  ]
}
