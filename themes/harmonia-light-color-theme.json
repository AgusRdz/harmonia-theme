  "name": "Harmonia Light",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "class": "#4B5EBD",
    "decorator": "#4B5EBDD0",
    "enumMember": "#D4AB74",
    "function": "#4B5EBD",
    "interface": "#4B5EBD",
    "method": "#4B5EBD",
    "namespace": "#4B5EBD",
    "operator": "#4A4F62B3",
    "parameter": "#489DAC",
    "property": "#3B3F48",
    "property.declaration": "#4A4F62",
    "regexp": "#4AB5B4",
    "type": "#4B5EBD",
    "variable.readonly": "#D4AB74"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.block",
        "comment.line",
        "comment.line.double-slash"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#808890"
      }
    },
    {
      "name": "Comment Punctuation",
      "scope": ["punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#808890"
      }
    },
    {
      "name": "HTML Angle Brackets",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.html"
      ],
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "name": "JSX/TSX Angle Brackets",
      "scope": [
        "punctuation.definition.tag.jsx",
        "punctuation.definition.tag.tsx"
      ],
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "name": "HTML Tag Name",
      "scope": ["entity.name.tag", "support.class.component.html"],
      "settings": {
        "foreground": "#4B5EBD"
      }
    },
    {
      "name": "JSX/TSX Tag Name",
      "scope": ["entity.name.tag.jsx", "entity.name.tag.tsx"],
      "settings": {
        "foreground": "#4B5EBD"
      }
    },
    {
      "name": "Component Tag (JSX/TSX/Vue)",
      "scope": [
        "support.class.component",
        "support.class.component.jsx",
        "support.class.component.tsx",
        "support.class.component.vue"
      ],
      "settings": {
        "foreground": "#4B5EBD"
      }
    },
    {
      "name": "HTML/JSX/Vue Attributes",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.vue",
        "meta.directive.vue"
      ],
      "settings": {
        "foreground": "#3574A3"
      }
    },
    {
      "name": "Attribute Values (HTML/JSX/Vue)",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.quoted.double.jsx",
        "string.quoted.single.jsx",
        "string.quoted.double.tsx",
        "string.quoted.single.tsx",
        "string.quoted.double.vue",
        "string.quoted.single.vue"
      ],
      "settings": {
        "foreground": "#2F7C61"
      }
    },
    {
      "name": "Tailwind Utility Classes",
      "scope": [
        "string.quoted.double.html class",
        "string.quoted.single.html class"
      ],
      "settings": {
        "foreground": "#31836A"
      }
    },
    {
      "name": "HTML Text Content",
      "scope": ["text.html.basic", "text.html.derivative"],
      "settings": {
        "foreground": "#3B3F48"
      }
    },
    {
      "name": "JSX/TSX Text (Children)",
      "scope": [
        "meta.jsx.children",
        "source.jsx meta.jsx.children",
        "source.js meta.jsx.children",
        "source.tsx meta.jsx.children",
        "meta.tag.inline.jsx meta.jsx.children",
        "meta.tag.block.jsx meta.jsx.children",
        "meta.tag.inline.tsx meta.jsx.children",
        "meta.tag.block.tsx meta.jsx.children"
      ],
      "settings": {
        "foreground": "#3B3F48"
      }
    },
    {
      "name": "Functions",
      "scope": ["support.function", "entity.name.function"],
      "settings": {
        "foreground": "#4B5EBD"
      }
    },
    {
      "name": "Namespaces / Types / Classes",
      "scope": [
        "meta.use",
        "support.class",
        "entity.name.namespace",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#4B5EBD"
      }
    },
    {
      "name": "Strings (Generic)",
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#2F7C61"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "meta.definition.variable.name",
        "support.variable",
        "variable.other.php"
      ],
      "settings": {
        "foreground": "#4AB5B4"
      }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter", "meta.function.parameter"],
      "settings": {
        "foreground": "#489DAC"
      }
    },
    {
      "name": "Constants / Language / Character",
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#D4AB74"
      }
    },
    {
      "name": "Keywords & Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#A454CB"
      }
    },
    {
      "name": "Italicized Keywords & Operators",
      "scope": [
        "entity.name.method",
        "keyword.control.conditional",
        "keyword.control.flow",
        "keyword.control.loop",
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.default",
        "keyword.control.from",
        "keyword.control.switch",
        "keyword.operator.expression.delete",
        "keyword.operator.expression.in",
        "keyword.operator.expression.of",
        "keyword.operator.new",
        "keyword.operator.sizeof",
        "storage"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown: Heading",
      "scope": ["markup.heading"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#3a3a3a"
      }
    },
    {
      "name": "Markdown: Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#7b5ea1"
      }
    },
    {
      "name": "Markdown: Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#555f77"
      }
    },
    {
      "name": "Markdown: Inline Code",
      "scope": ["markup.inline.raw.markdown"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#44894a"
      }
    },
    {
      "name": "Markdown: Block Quote",
      "scope": ["markup.quote.markdown"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#4ab5b4"
      }
    },
    {
      "name": "Markdown: Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#76b685"
      }
    },
    {
      "name": "Markdown: Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#e0a96d"
      }
    },
    {
      "name": "Markdown: Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#ec7279"
      }
    },
    {
      "name": "Markdown: Separator",
      "scope": ["meta.separator.markdown"],
      "settings": {
        "foreground": "#cfd8e3"
      }
    },
    {
      "name": "Invalid / Illegal",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#ec7279"
      }
    },
    {
      "scope": ["punctuation.definition.keyword"],
      "settings": {
        "foreground": "#4B5EBD"
      }
    },
    {
      "scope": ["support.type.builtin"],
      "settings": {
        "foreground": "#4B5EBD"
      }
    },
    {
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#4AB5B4"
      }
    },
    {
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#3574A3"
      }
    },
    {
      "scope": ["meta.decorator"],
      "settings": {
        "foreground": "#4B5EBDD0"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#3B3F48"
      }
    },
    {
      "scope": ["punctuation.accessor"],
      "settings": {
        "foreground": "#4A4F6299"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#4A4F62B3"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#4AB5B4CC"
      }
    },
    {
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#76B685CC"
      }
    },
    {
      "name": "PHP use keyword",
      "scope": [
        "keyword.other.use",
        "keyword.other.namespace.use",
        "keyword.other.use.php"
      ],
      "settings": {
        "foreground": "#B99EF0"
      }
    },
    {
      "name": "Class and implements keywords",
      "scope": [
        "keyword.declaration.class",
        "storage.type.class",
        "keyword.declaration.interface",
        "keyword.other.implements",
        "storage.modifier.implements"
      ],
      "settings": {
        "foreground": "#B99EF0"
      }
    },
    {
      "name": "Type and namespace identifiers",
      "scope": [
        "entity.name.type",
        "entity.name.type.class",
        "entity.name.type.interface",
        "entity.other.inherited-class",
        "support.class",
        "support.type",
        "entity.name.namespace",
        "support.other.namespace"
      ],
      "settings": {
        "foreground": "#9CC0FF"
      }
    },
    {
      "name": "Namespace and punctuation separators",
      "scope": [
        "punctuation.separator.namespace",
        "punctuation.accessor",
        "punctuation.separator",
        "punctuation.separator.inheritance"
      ],
      "settings": {
        "foreground": "#C8CAD6"
      }
    },
    {
      "name": "this/self",
      "scope": ["variable.language.this", "variable.language.self"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9CB8FFCC"
      }
    },
    {
      "name": "PHP variable sigil matches variable color",
      "scope": [
        "punctuation.definition.variable.php",
        "variable.other.php punctuation.definition.variable"
      ],
      "settings": { "foreground": "#5A6275" }
    }
  ],
  "type": "light"
}
