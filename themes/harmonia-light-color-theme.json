{
  "$schema": "vscode://schemas/color-theme",
  "name": "Harmonia Light",
  "type": "light",
  "colors": {
    "activityBar.background": "#e6e8eb",
    "activityBar.border": "#00000000",
    "activityBar.foreground": "#3a3a3a",
    "activityBarBadge.background": "#c574dd",
    "activityBarBadge.foreground": "#ffffff",
    "badge.background": "#b678d6",
    "badge.foreground": "#ffffff",
    "breadcrumb.background": "#eceef1",
    "breadcrumb.focusForeground": "#4b5ebd",
    "breadcrumb.foreground": "#7a7f89",
    "button.background": "#C9A7EA",
    "button.border": "#00000000",
    "button.foreground": "#2e3440",
    "button.hoverBackground": "#B895E1",
    "diffEditor.border": "#d0d3d6",
    "diffEditor.insertedLineBackground": "#bde7cb20",
    "diffEditor.insertedTextBackground": "#489d6a66",
    "diffEditor.removedLineBackground": "#f2bcbc20",
    "diffEditor.removedTextBackground": "#b1464666",
    "dropdown.border": "#00000000",
    "editor.background": "#eceef1",
    "editor.focusedStackFrameHighlightBackground": "#00000000",
    "editor.foreground": "#2d2d2d",
    "editor.inactiveSelectionBackground": "#e3e8f0AA",
    "editor.lineHighlightBackground": "#e6eaf0",
    "editor.lineHighlightBorder": "#00000000",
    "editor.selectionBackground": "#d4dbe8",
    "editor.selectionForeground": "#2d2d2d",
    "editor.selectionHighlightBackground": "#d4dbe870",
    "editorBracketMatch.background": "#cfe8fc",
    "editorBracketMatch.border": "#8bb6e2",
    "editorCursor.foreground": "#3a3a3a",
    "editorGroup.border": "#d0d3d6",
    "editorGutter.addedBackground": "#86c591",
    "editorGutter.background": "#eceef1",
    "editorGutter.commentRangeForeground": "#83bde7",
    "editorGutter.deletedBackground": "#e78e8e",
    "editorGutter.modifiedBackground": "#83bde7",
    "editorIndentGuide.activeBackground1": "#c1c1c1",
    "editorIndentGuide.background1": "#e5e5e5",
    "editorLineNumber.foreground": "#7a7f89",
    "editorWhitespace.foreground": "#c3c7ce",
    "focusBorder": "#00000000",
    "gitDecoration.addedResourceForeground": "#7ebf87",
    "gitDecoration.conflictingResourceForeground": "#e07b56",
    "gitDecoration.deletedResourceForeground": "#d76585",
    "gitDecoration.ignoredResourceForeground": "#b4b8c2",
    "gitDecoration.modifiedResourceForeground": "#a454cb",
    "gitDecoration.renamedResourceForeground": "#6eaadb",
    "gitDecoration.submoduleResourceForeground": "#8a8fa4",
    "gitDecoration.untrackedResourceForeground": "#7ebf87",
    "list.activeSelectionBackground": "#dad3e8",
    "list.activeSelectionForeground": "#2e3440",
    "list.deemphasizedForeground": "#888ca0",
    "list.focusAndSelectionOutline": "#00000000",
    "list.focusOutline": "#00000000",
    "list.hoverBackground": "#e8e3f2",
    "list.inactiveSelectionBackground": "#e3dff0",
    "list.inactiveSelectionForeground": "#2e3440",
    "panel.background": "#eceef1",
    "panel.border": "#d0d3d6",
    "scmGraph.foreground1": "#7aa2f7",
    "scmGraph.foreground2": "#88c6c3",
    "scmGraph.foreground3": "#a3d9a5",
    "scmGraph.foreground4": "#c574dd",
    "scmGraph.foreground5": "#5a5f7a",
    "scmGraph.historyItemBaseRefColor": "#6eaadb",
    "scmGraph.historyItemHoverAdditionsForeground": "#7ebf87",
    "scmGraph.historyItemHoverDefaultLabelBackground": "#dcdfe3",
    "scmGraph.historyItemHoverDefaultLabelForeground": "#2e3440",
    "scmGraph.historyItemHoverDeletionsForeground": "#d76585",
    "scmGraph.historyItemHoverLabelForeground": "#2e3440",
    "scmGraph.historyItemRefColor": "#b678d6",
    "scmGraph.historyItemRemoteRefColor": "#7ebf87",
    "scrollbar.shadow": "#00000020",
    "sideBar.background": "#eceef1",
    "sideBar.border": "#d0d3d6",
    "sideBar.foreground": "#3a3a3a",
    "statusBar.background": "#d0d3d6",
    "statusBar.border": "#00000000",
    "statusBar.debuggingBackground": "#c574dd",
    "statusBar.debuggingForeground": "#ffffff",
    "statusBar.foreground": "#3a3a3a",
    "statusBar.noFolderBackground": "#d5d8db",
    "statusBar.noFolderForeground": "#3a3a3a",
    "statusBarItem.remoteBackground": "#00000000",
    "statusBarItem.remoteForeground": "#2c2e40",
    "tab.activeBackground": "#e6e8eb",
    "tab.activeForeground": "#3a3a3a",
    "tab.border": "#d0d3d6",
    "tab.inactiveBackground": "#e2e4e7",
    "tab.inactiveForeground": "#7d8590",
    "terminal.ansiBlack": "#9FA6B2",
    "terminal.ansiBlue": "#6EAADB",
    "terminal.ansiBrightBlack": "#808890",
    "terminal.ansiBrightBlue": "#8CA9D5",
    "terminal.ansiBrightCyan": "#489DAC",
    "terminal.ansiBrightGreen": "#8FD0A0",
    "terminal.ansiBrightMagenta": "#B678D6",
    "terminal.ansiBrightRed": "#F19196",
    "terminal.ansiBrightWhite": "#3A3A3A",
    "terminal.ansiBrightYellow": "#DCBFA8",
    "terminal.ansiCyan": "#4AB5B4",
    "terminal.ansiGreen": "#7EBF87",
    "terminal.ansiMagenta": "#A454CB",
    "terminal.ansiRed": "#EC7279",
    "terminal.ansiWhite": "#2D2D2D",
    "terminal.ansiYellow": "#D4AB74",
    "terminal.background": "#eceef1",
    "terminal.border": "#d0d3d6",
    "terminal.findMatchBackground": "#CFE8FC66",
    "terminal.findMatchHighlightBackground": "#CFE8FC33",
    "terminal.foreground": "#2d2d2d",
    "terminal.hoverHighlightBackground": "#E6EAF0",
    "terminal.selectionBackground": "#D4DBE8",
    "terminal.tab.activeBorder": "#A454CB",
    "terminalCursor.foreground": "#3A3A3A"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#808890", "fontStyle": "italic" }
    },
    {
      "name": "HTML Angle Brackets",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.html"
      ],
      "settings": { "foreground": "#999999" }
    },
    {
      "name": "JSX/TSX Angle Brackets",
      "scope": [
        "punctuation.definition.tag.jsx",
        "punctuation.definition.tag.tsx"
      ],
      "settings": { "foreground": "#999999" }
    },
    {
      "name": "HTML Tag Name",
      "scope": ["entity.name.tag", "support.class.component.html"],
      "settings": { "foreground": "#4B5EBD" }
    },
    {
      "name": "JSX/TSX Tag Name",
      "scope": ["entity.name.tag.jsx", "entity.name.tag.tsx"],
      "settings": { "foreground": "#4B5EBD" }
    },
    {
      "name": "Component Tag (JSX/TSX/Vue)",
      "scope": [
        "support.class.component",
        "support.class.component.jsx",
        "support.class.component.tsx",
        "support.class.component.vue"
      ],
      "settings": { "foreground": "#4B5EBD" }
    },
    {
      "name": "HTML/JSX/Vue Attributes",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.vue",
        "meta.directive.vue"
      ],
      "settings": { "foreground": "#3574A3" }
    },
    {
      "name": "Tailwind Utility Classes",
      "scope": [
        "string.quoted.double.html class",
        "string.quoted.single.html class",
        "meta.attribute.class.html string.quoted",
        "meta.attribute.class.vue string.quoted"
      ],
      "settings": { "foreground": "#31836A" }
    },
    {
      "name": "Attribute Values (HTML/JSX/Vue)",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.quoted.double.jsx",
        "string.quoted.single.jsx",
        "string.quoted.double.tsx",
        "string.quoted.single.tsx",
        "string.quoted.double.vue",
        "string.quoted.single.vue"
      ],
      "settings": { "foreground": "#2F7C61" }
    },
    {
      "name": "HTML Text Content",
      "scope": ["text.html.basic", "text.html.derivative"],
      "settings": { "foreground": "#3B3F48" }
    },
    {
      "name": "JSX/TSX Text (Children)",
      "scope": [
        "meta.jsx.children",
        "source.jsx meta.jsx.children",
        "source.js meta.jsx.children",
        "source.tsx meta.jsx.children",
        "meta.tag.inline.jsx meta.jsx.children",
        "meta.tag.block.jsx meta.jsx.children",
        "meta.tag.inline.tsx meta.jsx.children",
        "meta.tag.block.tsx meta.jsx.children"
      ],
      "settings": { "foreground": "#3B3F48" }
    },
    {
      "name": "Functions",
      "scope": ["support.function", "entity.name.function"],
      "settings": { "foreground": "#4B5EBD" }
    },
    {
      "name": "Namespaces / Types / Classes",
      "scope": [
        "meta.use",
        "support.class",
        "entity.name.namespace",
        "entity.name.type"
      ],
      "settings": { "foreground": "#4b5ebd" }
    },
    {
      "name": "Strings (Generic)",
      "scope": ["string", "punctuation.definition.string"],
      "settings": { "foreground": "#2f7c61" }
    },
    {
      "name": "Variables",
      "scope": ["variable", "meta.definition.variable.name"],
      "settings": { "foreground": "#4ab5b4" }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter", "meta.function.parameter"],
      "settings": { "foreground": "#489dac" }
    },
    {
      "name": "Constants / Language / Character",
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": { "foreground": "#D4AB74" }
    },
    {
      "name": "Keywords & Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#A454CB" }
    },
    {
      "name": "Italicized Keywords & Operators",
      "scope": [
        "entity.name.method",
        "keyword.control.conditional",
        "keyword.control.flow",
        "keyword.control.loop",
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.default",
        "keyword.control.from",
        "keyword.control.switch",
        "keyword.operator.expression.delete",
        "keyword.operator.expression.in",
        "keyword.operator.expression.of",
        "keyword.operator.new",
        "keyword.operator.sizeof",
        "storage"
      ],
      "settings": { "fontStyle": "italic" }
    },
    {
      "name": "Markdown: Heading",
      "scope": ["markup.heading"],
      "settings": { "foreground": "#3a3a3a", "fontStyle": "bold" }
    },
    {
      "name": "Markdown: Bold",
      "scope": ["markup.bold"],
      "settings": { "foreground": "#7b5ea1", "fontStyle": "bold" }
    },
    {
      "name": "Markdown: Italic",
      "scope": ["markup.italic"],
      "settings": { "foreground": "#555f77", "fontStyle": "italic" }
    },
    {
      "name": "Markdown: Inline Code",
      "scope": ["markup.inline.raw.markdown"],
      "settings": { "foreground": "#44894a", "fontStyle": "italic" }
    },
    {
      "name": "Markdown: Block Quote",
      "scope": ["markup.quote.markdown"],
      "settings": { "foreground": "#4ab5b4", "fontStyle": "italic" }
    },
    {
      "name": "Markdown: Inserted",
      "scope": ["markup.inserted"],
      "settings": { "foreground": "#76b685" }
    },
    {
      "name": "Markdown: Changed",
      "scope": ["markup.changed"],
      "settings": { "foreground": "#e0a96d" }
    },
    {
      "name": "Markdown: Deleted",
      "scope": ["markup.deleted"],
      "settings": { "foreground": "#ec7279" }
    },
    {
      "name": "Markdown: Separator",
      "scope": ["meta.separator.markdown"],
      "settings": { "foreground": "#cfd8e3" }
    },
    {
      "name": "Invalid / Illegal",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ec7279" }
    }
  ]
}
