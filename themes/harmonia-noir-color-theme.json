{
  "$schema": "vscode://schemas/color-theme",
  "name": "Harmonia Noir",
  "type": "dark",
  "colors": {
    "activityBar.background": "#1A1822",
    "activityBar.border": "#00000000",
    "activityBar.foreground": "#d0d0e0",
    "activityBarBadge.background": "#b88ce8",
    "activityBarBadge.foreground": "#14121A",
    "badge.background": "#9a78cc",
    "badge.foreground": "#e6e6f0",
    "breadcrumb.background": "#181825",
    "breadcrumb.focusForeground": "#b8bdf0",
    "breadcrumb.foreground": "#777a96",
    "button.background": "#A98BE6",
    "button.foreground": "#14121A",
    "button.hoverBackground": "#9D7DDC",
    "diffEditor.border": "#5a5f7a",
    "diffEditor.insertedLineBackground": "#a3d9a520",
    "diffEditor.insertedTextBackground": "#1a512a88",
    "diffEditor.removedLineBackground": "#ec727920",
    "diffEditor.removedTextBackground": "#6e1d1d88",
    "dropdown.border": "#00000000",
    "editor.background": "#14121A",
    "editor.foreground": "#d0d0e0",
    "editor.inactiveSelectionBackground": "#27243aAA",
    "editor.lineHighlightBackground": "#1C1A25",
    "editor.lineHighlightBorder": "#232132",
    "editor.selectionBackground": "#312E45",
    "editor.selectionForeground": "#ffffff",
    "editor.selectionHighlightBackground": "#4a3f6a80",
    "editorBracketMatch.background": "#3f3a58",
    "editorBracketMatch.border": "#5a5f7a",
    "editorCursor.foreground": "#bba0f5",
    "editorGroup.border": "#2a2840",
    "editorGutter.addedBackground": "#a3d9a5",
    "editorGutter.background": "#14121A",
    "editorGutter.commentRangeForeground": "#8bd4e9",
    "editorGutter.deletedBackground": "#ec7279",
    "editorGutter.modifiedBackground": "#6ebddb",
    "editorIndentGuide.activeBackground1": "#4C4665",
    "editorIndentGuide.background1": "#2A2835",
    "editorLineNumber.foreground": "#4c4f68",
    "editorWhitespace.foreground": "#2a2c3a",
    "focusBorder": "#00000000",
    "gitDecoration.addedResourceForeground": "#a3d9a5",
    "gitDecoration.conflictingResourceForeground": "#e0a96d",
    "gitDecoration.deletedResourceForeground": "#ec7279",
    "gitDecoration.ignoredResourceForeground": "#5a5f7a",
    "gitDecoration.modifiedResourceForeground": "#b88ce8",
    "gitDecoration.renamedResourceForeground": "#6ebddb",
    "gitDecoration.submoduleResourceForeground": "#94E2D5",
    "gitDecoration.untrackedResourceForeground": "#a3d9a5",
    "list.activeSelectionBackground": "#2e2a46",
    "list.activeSelectionForeground": "#e0e0f0",
    "list.deemphasizedForeground": "#888ca0",
    "list.focusAndSelectionOutline": "#00000000",
    "list.focusOutline": "#00000000",
    "list.hoverBackground": "#2b2742",
    "list.inactiveSelectionBackground": "#25213a",
    "list.inactiveSelectionForeground": "#d0d0e0",
    "panel.background": "#14121A",
    "panel.border": "#2a2840",
    "scmGraph.foreground1": "#b88ce8",
    "scmGraph.foreground2": "#ef874b",
    "scmGraph.foreground3": "#8bd4e9",
    "scmGraph.foreground4": "#a3d9a5",
    "scmGraph.foreground5": "#888ca0",
    "scmGraph.historyItemBaseRefColor": "#6ebddb",
    "scmGraph.historyItemHoverAdditionsForeground": "#83b987",
    "scmGraph.historyItemHoverDefaultLabelBackground": "#25213a",
    "scmGraph.historyItemHoverDefaultLabelForeground": "#e6e6f0",
    "scmGraph.historyItemHoverDeletionsForeground": "#de7d84",
    "scmGraph.historyItemHoverLabelForeground": "#e6e6f0",
    "scmGraph.historyItemRefColor": "#b88ce8",
    "scmGraph.historyItemRemoteRefColor": "#83b987",
    "scrollbar.shadow": "#00000050",
    "sideBar.background": "#14121A",
    "sideBar.border": "#00000000",
    "sideBar.foreground": "#d0d0e0",
    "statusBar.background": "#1A1822",
    "statusBar.debuggingBackground": "#b88ce8",
    "statusBar.debuggingForeground": "#14121A",
    "statusBar.foreground": "#d0d0e0",
    "statusBar.noFolderBackground": "#1A1822",
    "statusBar.noFolderForeground": "#d0d0e0",
    "statusBarItem.remoteBackground": "#00000000",
    "statusBarItem.remoteForeground": "#e6e6f0",
    "tab.activeBackground": "#14121A",
    "tab.activeForeground": "#ffffff",
    "tab.border": "#2a2840",
    "tab.inactiveBackground": "#1A1822",
    "tab.inactiveForeground": "#777a96",
    "terminal.ansiBlack": "#2a2b36",
    "terminal.ansiBlue": "#7ea8e6",
    "terminal.ansiBrightBlack": "#5a5f7a",
    "terminal.ansiBrightBlue": "#9cc7ff",
    "terminal.ansiBrightCyan": "#a3dee0",
    "terminal.ansiBrightGreen": "#a8e0db",
    "terminal.ansiBrightMagenta": "#caa7f2",
    "terminal.ansiBrightRed": "#ec8a8f",
    "terminal.ansiBrightWhite": "#ffffff",
    "terminal.ansiBrightYellow": "#e3bb86",
    "terminal.ansiCyan": "#88c6c3",
    "terminal.ansiGreen": "#9fd6d1",
    "terminal.ansiMagenta": "#bba0f5",
    "terminal.ansiRed": "#ec7279",
    "terminal.ansiWhite": "#d0d0e0",
    "terminal.ansiYellow": "#d4aa76",
    "terminal.background": "#14121A",
    "terminal.border": "#2a2840",
    "terminal.foreground": "#d0d0e0",
    "textLink.foreground": "#b88ce8",
    "textLink.activeForeground": "#9fd6d1"
  },
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.block",
        "comment.line",
        "comment.line.double-slash"
      ],
      "settings": { "foreground": "#9ca3af", "fontStyle": "italic" }
    },
    {
      "name": "Comment Punctuation",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#5a5f7a", "fontStyle": "italic" }
    },
    {
      "name": "HTML Angle Brackets",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.html"
      ],
      "settings": { "foreground": "#b0b0b0" }
    },
    {
      "name": "JSX/TSX Angle Brackets",
      "scope": [
        "punctuation.definition.tag.jsx",
        "punctuation.definition.tag.tsx"
      ],
      "settings": { "foreground": "#b0b0b0" }
    },
    {
      "name": "HTML Tag Name",
      "scope": ["entity.name.tag", "support.class.component.html"],
      "settings": { "foreground": "#dcbfa8" }
    },
    {
      "name": "JSX/TSX Tag Name",
      "scope": ["entity.name.tag.jsx", "entity.name.tag.tsx"],
      "settings": { "foreground": "#dcbfa8" }
    },
    {
      "name": "Component Tag (JSX/TSX/Vue)",
      "scope": [
        "support.class.component",
        "support.class.component.jsx",
        "support.class.component.tsx",
        "support.class.component.vue"
      ],
      "settings": { "foreground": "#b8bdf0" }
    },
    {
      "name": "HTML/JSX/Vue Attributes",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.vue",
        "meta.directive.vue"
      ],
      "settings": { "foreground": "#8ca9d5" }
    },
    {
      "name": "Tailwind Utility Classes",
      "scope": [
        "string.quoted.double.html class",
        "string.quoted.single.html class"
      ],
      "settings": { "foreground": "#9fd6d1" }
    },
    {
      "name": "Attribute Values (HTML/JSX/Vue)",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.quoted.double.jsx",
        "string.quoted.single.jsx",
        "string.quoted.double.tsx",
        "string.quoted.single.tsx",
        "string.quoted.double.vue",
        "string.quoted.single.vue"
      ],
      "settings": { "foreground": "#88c6c3" }
    },
    {
      "name": "HTML Text Content",
      "scope": ["text.html.basic", "text.html.derivative"],
      "settings": { "foreground": "#e2e2ec" }
    },
    {
      "name": "JSX/TSX Text (Children)",
      "scope": [
        "meta.jsx.children",
        "source.jsx meta.jsx.children",
        "source.js meta.jsx.children",
        "source.tsx meta.jsx.children",
        "meta.tag.inline.jsx meta.jsx.children",
        "meta.tag.block.jsx meta.jsx.children",
        "meta.tag.inline.tsx meta.jsx.children",
        "meta.tag.block.tsx meta.jsx.children"
      ],
      "settings": { "foreground": "#e2e2ec" }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#9db1ff" }
    },
    {
      "name": "Namespaces / Types / Classes",
      "scope": [
        "entity.name.namespace",
        "entity.name.type",
        "meta.use",
        "support.class"
      ],
      "settings": { "foreground": "#b8bdf0" }
    },
    {
      "name": "Strings (Generic)",
      "scope": ["punctuation.definition.string", "string"],
      "settings": { "foreground": "#9fd6d1" }
    },
    {
      "name": "Variables",
      "scope": ["meta.definition.variable.name", "variable"],
      "settings": { "foreground": "#88c6c3" }
    },
    {
      "name": "Parameters",
      "scope": ["meta.function.parameter", "variable.parameter"],
      "settings": { "foreground": "#94E2D5" }
    },
    {
      "name": "Constants / Language / Character",
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": { "foreground": "#e0a96d" }
    },
    {
      "name": "Keywords & Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#bba0f5" }
    },
    {
      "name": "Italicized Keywords & Operators",
      "scope": [
        "entity.name.method",
        "keyword.control.conditional",
        "keyword.control.flow",
        "keyword.control.loop",
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.default",
        "keyword.control.from",
        "keyword.control.switch",
        "keyword.operator.expression.delete",
        "keyword.operator.expression.in",
        "keyword.operator.expression.of",
        "keyword.operator.new",
        "keyword.operator.sizeof",
        "storage"
      ],
      "settings": { "fontStyle": "italic" }
    },
    {
      "name": "Markdown: Heading",
      "scope": ["markup.heading"],
      "settings": { "foreground": "#D9E0EE", "fontStyle": "bold" }
    },
    {
      "name": "Markdown: Bold",
      "scope": ["markup.bold"],
      "settings": { "foreground": "#caa7f2", "fontStyle": "bold" }
    },
    {
      "name": "Markdown: Italic",
      "scope": ["markup.italic"],
      "settings": { "foreground": "#BAC2DE", "fontStyle": "italic" }
    },
    {
      "name": "Markdown: Inline Code",
      "scope": ["markup.inline.raw.markdown"],
      "settings": { "foreground": "#A6E3A1", "fontStyle": "italic" }
    },
    {
      "name": "Markdown: Block Quote",
      "scope": ["markup.quote.markdown"],
      "settings": { "foreground": "#94E2D5", "fontStyle": "italic" }
    },
    {
      "name": "Markdown: Inserted",
      "scope": ["markup.inserted"],
      "settings": { "foreground": "#a3d9a5" }
    },
    {
      "name": "Markdown: Changed",
      "scope": ["markup.changed"],
      "settings": { "foreground": "#e0a96d" }
    },
    {
      "name": "Markdown: Deleted",
      "scope": ["markup.deleted"],
      "settings": { "foreground": "#ec7279" }
    },
    {
      "name": "Markdown: Separator",
      "scope": ["meta.separator.markdown"],
      "settings": { "foreground": "#5a5f7a" }
    },
    {
      "name": "Invalid / Illegal",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#ec7279" }
    }
  ]
}
