    "breadcrumb.background": "#000000",
    "breadcrumb.focusForeground": "#B99EF0",
    "breadcrumb.foreground": "#7B8094",
    "button.background": "#B99EF020",
    "button.border": "#0A0B0E",
    "button.foreground": "#E2E2E8",
    "button.hoverBackground": "#B99EF033",
    "dropdown.background": "#0C0D12",
    "dropdown.border": "#0A0B0E",
    "dropdown.foreground": "#E2E2E8",
    "editor.background": "#000000",
    "editor.findMatchBackground": "#FFB74D66",
    "editor.findMatchHighlightBackground": "#FFB74D33",
    "editor.findRangeHighlightBackground": "#0C0D1280",
    "editor.foldBackground": "#0A0B0E40",
    "editor.foreground": "#E2E2E8",
    "editor.hoverHighlightBackground": "#00BFFF1F",
    "editor.inactiveSelectionBackground": "#00BFFF33",
    "editor.lineHighlightBackground": "#FFFFFF0A",
    "editor.lineHighlightBorder": "#0F1014",
    "editor.linkedEditingBackground": "#B99EF015",
    "editor.selectionBackground": "#00BFFF66",
    "editor.selectionForeground": "#FFFFFF",
    "editor.selectionHighlightBackground": "#00BFFF26",
    "editor.wordHighlightBackground": "#00E5FF1F",
    "editor.wordHighlightStrongBackground": "#B99EF040",
    "editorBracketMatch.background": "#26283A",
    "editorBracketMatch.border": "#5A5F7A",
    "editorCursor.foreground": "#DCDCF0",
    "editorGhostText.foreground": "#C6CBDB",
    "editorGroup.border": "#0A0B0E",
    "editorGroupHeader.border": "#0A0B0E",
    "editorGroupHeader.noTabsBackground": "#0A0B0E",
    "editorGroupHeader.tabsBackground": "#0A0B0E",
    "editorHoverWidget.background": "#0B0C10",
    "editorHoverWidget.border": "#0A0B0E",
    "editorIndentGuide.activeBackground1": "#5A5F7A",
    "editorIndentGuide.background1": "#161821",
    "editorLineNumber.activeForeground": "#E1B37A",
    "editorLineNumber.foreground": "#4C4F68",
    "editorSuggestWidget.background": "#0B0C10",
    "editorSuggestWidget.border": "#0A0B0E",
    "editorSuggestWidget.foreground": "#C8C8D2",
    "editorSuggestWidget.highlightForeground": "#E2E2E8",
    "editorSuggestWidget.selectedBackground": "#111218",
    "editorWhitespace.foreground": "#161821",
    "editorWidget.background": "#0B0C10",
    "editorWidget.border": "#0A0B0E",
    "editorWidget.foreground": "#E2E2E8",
    "focusBorder": "#00000000",
    "foreground": "#E2E2E8",
    "gitDecoration.addedResourceForeground": "#A7DEAE",
    "gitDecoration.conflictingResourceForeground": "#E1B37A",
    "gitDecoration.deletedResourceForeground": "#E8848A",
    "gitDecoration.ignoredResourceForeground": "#5A5F7A",
    "gitDecoration.modifiedResourceForeground": "#E1B37A",
    "gitDecoration.renamedResourceForeground": "#96D7D4",
    "gitDecoration.submoduleResourceForeground": "#C5A6F2",
    "gitDecoration.untrackedResourceForeground": "#D6D6DE",
    "input.background": "#141414",
    "input.border": "#1E1E1E",
    "input.foreground": "#E2E2E8",
    "input.placeholderForeground": "#A8A8B8",
    "inputOption.activeBackground": "#0C0D12",
    "inputOption.activeBorder": "#0A0B0E",
    "list.activeSelectionBackground": "#0C0D12",
    "list.activeSelectionForeground": "#E6E6F0",
    "list.deemphasizedForeground": "#888CA0",
    "list.focusAndSelectionOutline": "#00000000",
    "list.focusOutline": "#00000000",
    "list.highlightForeground": "#B99EF0",
    "list.hoverBackground": "#0C0D12",
    "list.inactiveSelectionBackground": "#0A0B0E",
    "list.inactiveSelectionForeground": "#DADAE4",
    "menubar.selectionBackground": "#0C0D12",
    "menubar.selectionForeground": "#E2E2E8",
    "minimap.background": "#000000",
    "minimap.selectionHighlight": "#00BFFF80",
    "panel.background": "#000000",
    "panel.border": "#0A0B0E",
    "panelTitle.activeBorder": "#B99EF0",
    "panelTitle.activeForeground": "#E2E2E8",
    "panelTitle.inactiveForeground": "#7B8094",
    "peekView.border": "#5A5F7A",
    "peekViewEditor.background": "#000000",
    "peekViewResult.background": "#0A0B0E",
    "peekViewTitle.background": "#0A0B0E",
    "quickInput.background": "#000000",
    "quickInput.foreground": "#E2E2E8",
    "quickInputList.focusBackground": "#14141A",
    "quickInputList.focusForeground": "#FFFFFF",
    "quickInputList.focusIconForeground": "#8BD4E9",
    "quickInputTitle.background": "#050508",
    "scmGraph.historyItemHoverLabelForeground": "#000000",
    "scmGraph.historyItemRefColor": "#8BD4E9",
    "scrollbar.shadow": "#00000080",
    "scrollbarSlider.activeBackground": "#FFFFFF4D",
    "scrollbarSlider.background": "#FFFFFF1A",
    "scrollbarSlider.hoverBackground": "#FFFFFF33",
    "sideBar.background": "#000000",
    "sideBar.border": "#0A0B0E",
    "sideBar.foreground": "#DADAE4",
    "sideBarSectionHeader.background": "#000000",
    "sideBarSectionHeader.border": "#000000",
    "sideBarSectionHeader.foreground": "#DADBEA",
    "sideBarTitle.foreground": "#E2E2E8",
    "statusBar.background": "#000000",
    "statusBar.border": "#0A0B0E",
    "statusBar.debuggingBackground": "#E1B37A",
    "statusBar.debuggingForeground": "#0A0B0E",
    "statusBar.foreground": "#E2E2E8",
    "statusBar.noFolderBackground": "#0A0B0E",
    "statusBar.noFolderForeground": "#E2E2E8",
    "statusBarItem.hoverBackground": "#0C0D12",
    "statusBarItem.remoteBackground": "#000000",
    "statusBarItem.remoteForeground": "#E2E2E8",
    "tab.activeBackground": "#000000",
    "tab.activeBorderTop": "#B99EF0",
    "tab.activeForeground": "#FFFFFF",
    "tab.border": "#0A0B0E",
    "tab.hoverBackground": "#0C0D12",
    "tab.inactiveBackground": "#0A0B0E",
    "tab.inactiveForeground": "#7B8094",
    "tab.unfocusedHoverBackground": "#0A0B0E",
    "terminal.ansiBlack": "#111218",
    "terminal.ansiBlue": "#7AA2F7",
    "terminal.ansiBrightBlack": "#5A5F7A",
    "terminal.ansiBrightBlue": "#9CC0FF",
    "terminal.ansiBrightCyan": "#B3E4E1",
    "terminal.ansiBrightGreen": "#BEE9C3",
    "terminal.ansiBrightMagenta": "#D7BDF8",
    "terminal.ansiBrightRed": "#F0A2A6",
    "terminal.ansiBrightWhite": "#FFFFFF",
    "terminal.ansiBrightYellow": "#F0C894",
    "terminal.ansiCyan": "#96D7D4",
    "terminal.ansiGreen": "#A7DEAE",
    "terminal.ansiMagenta": "#C5A6F2",
    "terminal.ansiRed": "#E8848A",
    "terminal.ansiWhite": "#D6D6DE",
    "terminal.ansiYellow": "#E1B37A",
    "terminal.background": "#000000",
    "terminal.border": "#0A0B0E",
    "terminal.findMatchBackground": "#C7B4FF33",
    "terminal.findMatchHighlightBackground": "#B99EF022",
    "terminal.foreground": "#E2E2E8",
    "terminal.selectionBackground": "#00BFFF66",
    "textLink.activeForeground": "#b88ce8",
    "textLink.foreground": "#8bd4e9",
    "titleBar.activeBackground": "#000000",
    "titleBar.activeForeground": "#E2E2E8",
    "titleBar.inactiveBackground": "#0A0B0E",
    "titleBar.inactiveForeground": "#7B8094"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.block",
        "comment.line",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9CA3AF"
      }
    },
    {
      "name": "Keywords & Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "punctuation.definition.keyword"
      ],
      "settings": {
        "foreground": "#B99EF0"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#B8BDF0"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "meta.definition.variable.name",
        "support.variable"
      ],
      "settings": {
        "foreground": "#96D7D4"
      }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter", "meta.function.parameters variable"],
      "settings": {
        "foreground": "#A7DEAE"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string"],
      "settings": {
        "foreground": "#A7DEAE"
      }
    },
    {
      "name": "Numbers / Constants",
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#E1B37A"
      }
    },
    {
      "name": "Boolean / Null",
      "scope": ["constant.language.boolean", "constant.language.null"],
      "settings": {
        "foreground": "#E1B37A"
      }
    },
    {
      "name": "Punctuation / Delimiters",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.delimiter",
        "punctuation.separator",
        "punctuation.section"
      ],
      "settings": {
        "foreground": "#5A5F7A"
      }
    },
    {
      "name": "HTML/JSX Tag Name",
      "scope": [
        "entity.name.tag",
        "entity.name.tag.jsx",
        "entity.name.tag.tsx"
      ],
      "settings": {
        "foreground": "#D9C2A6"
      }
    },
    {
      "name": "Component Tags",
      "scope": [
        "support.class.component",
        "support.class.component.jsx",
        "support.class.component.tsx",
        "support.class.component.vue"
      ],
      "settings": {
        "foreground": "#B8BDF0"
      }
    },
    {
      "name": "Angle Brackets",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.jsx",
        "punctuation.definition.tag.tsx"
      ],
      "settings": {
        "foreground": "#B0B0B0"
      }
    },
    {
      "name": "HTML/JSX/Vue Attributes",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.vue",
        "meta.directive.vue"
      ],
      "settings": {
        "foreground": "#8FAED8"
      }
    },
    {
      "name": "JSX/TSX Text (Children)",
      "scope": [
        "meta.jsx.children",
        "source.jsx meta.jsx.children",
        "source.tsx meta.jsx.children",
        "meta.tag.inline.jsx meta.jsx.children",
        "meta.tag.block.jsx meta.jsx.children",
        "meta.tag.inline.tsx meta.jsx.children",
        "meta.tag.block.tsx meta.jsx.children"
      ],
      "settings": {
        "foreground": "#E2E2E8"
      }
    },
    {
      "name": "CSS Tag / Type Selector",
      "scope": [
        "entity.name.tag.css",
        "source.css entity.name.tag",
        "meta.selector.css entity.name.tag",
        "meta.selector.scss entity.name.tag",
        "meta.selector.less entity.name.tag"
      ],
      "settings": {
        "foreground": "#D9C2A6"
      }
    },
    {
      "name": "CSS Class Selector",
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.scss",
        "entity.other.attribute-name.class.less",
        "meta.selector.css entity.other.attribute-name.class",
        "meta.selector.scss entity.other.attribute-name.class",
        "meta.selector.less entity.other.attribute-name.class"
      ],
      "settings": {
        "foreground": "#8FAED8"
      }
    },
    {
      "name": "CSS ID Selector",
      "scope": [
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.id.scss",
        "entity.other.attribute-name.id.less"
      ],
      "settings": {
        "foreground": "#7AA2F7"
      }
    },
    {
      "name": "CSS Pseudo (class/element)",
      "scope": [
        "source.css entity.other.attribute-name.pseudo-class",
        "source.css entity.other.attribute-name.pseudo-element",
        "punctuation.definition.pseudo-class.css",
        "punctuation.definition.pseudo-element.css"
      ],
      "settings": {
        "foreground": "#B99EF0"
      }
    },
    {
      "name": "CSS Property Name",
      "scope": [
        "support.type.property-name.css",
        "source.css support.type.property-name"
      ],
      "settings": {
        "foreground": "#B8BDF0"
      }
    },
    {
      "name": "CSS Property Value / Strings",
      "scope": [
        "support.constant.property-value.css",
        "source.css string.quoted",
        "source.css variable.parameter.url"
      ],
      "settings": {
        "foreground": "#A7DEAE"
      }
    },
    {
      "name": "CSS Numbers & Units",
      "scope": ["source.css constant.numeric", "source.css keyword.other.unit"],
      "settings": {
        "foreground": "#E1B37A"
      }
    },
    {
      "name": "CSS Functions (url(), var(), rgb(), …)",
      "scope": ["source.css support.function.misc"],
      "settings": {
        "foreground": "#96D7D4"
      }
    },
    {
      "name": "CSS Custom Property (name)",
      "scope": ["source.css entity.other.custom-property.name"],
      "settings": {
        "foreground": "#96D7D4"
      }
    },
    {
      "name": "CSS Braces / Punctuation / Commas",
      "scope": [
        "source.css punctuation.section",
        "source.css punctuation.terminator.rule",
        "punctuation.separator.list.comma.css"
      ],
      "settings": {
        "foreground": "#B0B0B0"
      }
    },
    {
      "name": "CSS Parent Selector (&)",
      "scope": ["entity.other.attribute-name.parent-selector"],
      "settings": {
        "foreground": "#B99EF0"
      }
    },
    {
      "name": "Tailwind utility classes (HTML string)",
      "scope": [
        "string.quoted.double.html class",
        "string.quoted.single.html class"
      ],
      "settings": {
        "foreground": "#A7DEAE"
      }
    },
    {
      "name": "Italicized Keywords & Operators",
      "scope": [
        "entity.name.method",
        "keyword.control.conditional",
        "keyword.control.flow",
        "keyword.control.loop",
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.default",
        "keyword.control.from",
        "keyword.control.switch",
        "keyword.operator.expression.delete",
        "keyword.operator.expression.in",
        "keyword.operator.expression.of",
        "keyword.operator.new",
        "keyword.operator.sizeof",
        "storage"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown: Heading",
      "scope": ["markup.heading"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#D9E0EE"
      }
    },
    {
      "name": "Markdown: Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#C5A6F2"
      }
    },
    {
      "name": "Markdown: Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#BAC2DE"
      }
    },
    {
      "name": "Markdown: Inline Code",
      "scope": ["markup.inline.raw", "markup.inline.raw.markdown"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#A7DEAE"
      }
    },
    {
      "name": "Markdown: Block Quote",
      "scope": ["markup.quote.markdown"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#96D7D4"
      }
    },
    {
      "name": "Markdown: Inserted/Changed/Deleted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#A7DEAE"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#E1B37A"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#E8848A"
      }
    },
    {
      "name": "Markdown: Separator",
      "scope": ["meta.separator.markdown"],
      "settings": {
        "foreground": "#5A5F7A"
      }
    },
    {
      "name": "Invalid / Illegal",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#E8848A"
      }
    }
  ]
}
