{
  "$schema": "vscode://schemas/color-theme",
  "name": "Harmonia Paper White",
  "type": "light",
  "semanticHighlighting": true,
  "colors": {
    "activityBar.background": "#FDFDFD",
    "activityBar.border": "#00000000",
    "activityBar.foreground": "#1A1A1A",
    "activityBarBadge.background": "#666666",
    "activityBarBadge.foreground": "#FFFFFF",
    "badge.background": "#888888",
    "badge.foreground": "#FFFFFF",
    "breadcrumb.background": "#FFFFFF",
    "breadcrumb.foreground": "#444444",
    "breadcrumb.focusForeground": "#1A1A1A",
    "button.background": "#E0E0E0",
    "button.foreground": "#1A1A1A",
    "button.hoverBackground": "#D5D5D5",
    "button.border": "#00000000",
    "dropdown.background": "#FFFFFF",
    "dropdown.border": "#DDDDDD",
    "dropdown.foreground": "#1A1A1A",
    "editor.background": "#FFFFFF",
    "editor.foreground": "#1A1A1A",
    "editor.lineHighlightBackground": "#F5F5F5",
    "editorCursor.foreground": "#000000",
    "editor.selectionBackground": "#DDEEFF",
    "editor.selectionForeground": "#000000",
    "editor.selectionHighlightBackground": "#CCDDEE55",
    "editor.findMatchBackground": "#CFE8FC66",
    "editor.findMatchHighlightBackground": "#CFE8FC33",
    "editor.findRangeHighlightBackground": "#E6F0FF55",
    "editor.hoverHighlightBackground": "#E6F0FF55",
    "editor.foldBackground": "#F2F6FF66",
    "editorBracketMatch.background": "#EEEEEE",
    "editorBracketMatch.border": "#4B5EBD",
    "editorGroup.border": "#E0E0E0",
    "editorGroupHeader.border": "#E0E0E0",
    "editorGroupHeader.tabsBackground": "#FAFAFA",
    "editorLineNumber.foreground": "#A9AEB6",
    "editorWhitespace.foreground": "#E2E6EC",
    "focusBorder": "#00000000",
    "gitDecoration.addedResourceForeground": "#2E7D32",
    "gitDecoration.modifiedResourceForeground": "#7A4EC7",
    "gitDecoration.deletedResourceForeground": "#C2410C",
    "gitDecoration.renamedResourceForeground": "#0E7490",
    "gitDecoration.untrackedResourceForeground": "#176C2B",
    "gitDecoration.ignoredResourceForeground": "#9AA1AE",
    "gitDecoration.conflictingResourceForeground": "#B26A00",
    "gitDecoration.submoduleResourceForeground": "#7B3FBF",
    "list.activeSelectionBackground": "#E8F0FF",
    "list.activeSelectionForeground": "#000000",
    "list.hoverBackground": "#F0F8FF",
    "list.inactiveSelectionBackground": "#F2F2F2",
    "list.inactiveSelectionForeground": "#000000",
    "panel.background": "#FFFFFF",
    "panel.border": "#E0E0E0",
    "scmGraph.foreground1": "#7B3FBF",
    "scmGraph.foreground2": "#B35B26",
    "scmGraph.foreground3": "#2C5DB0",
    "scmGraph.foreground4": "#2E7D32",
    "scmGraph.foreground5": "#9AA1AE",
    "scmGraph.historyItemBaseRefColor": "#2C5DB0",
    "scmGraph.historyItemHoverAdditionsForeground": "#2E7D32",
    "scmGraph.historyItemHoverDefaultLabelBackground": "#EDE9F9",
    "scmGraph.historyItemHoverDefaultLabelForeground": "#1E1E1E",
    "scmGraph.historyItemHoverDeletionsForeground": "#C2410C",
    "scmGraph.historyItemHoverLabelForeground": "#ffffff",
    "scmGraph.historyItemRefColor": "#7B3FBF",
    "scmGraph.historyItemRemoteRefColor": "#2E7D32",
    "sideBar.background": "#FAFAFA",
    "sideBar.border": "#E0E0E0",
    "sideBar.foreground": "#1A1A1A",
    "statusBar.background": "#FDFDFD",
    "statusBar.foreground": "#1A1A1A",
    "statusBar.debuggingBackground": "#D4AB74",
    "statusBar.debuggingForeground": "#1A1A1A",
    "tab.activeBackground": "#FFFFFF",
    "tab.activeForeground": "#1A1A1A",
    "tab.border": "#E0E0E0",
    "tab.inactiveBackground": "#F8F8F8",
    "tab.inactiveForeground": "#555555",
    "terminal.background": "#FFFFFF",
    "terminal.foreground": "#2D2D2D",
    "terminalCursor.foreground": "#111827",
    "terminal.ansiBlack": "#9FA6B2",
    "terminal.ansiRed": "#EC7279",
    "terminal.ansiGreen": "#7EBF87",
    "terminal.ansiYellow": "#D4AB74",
    "terminal.ansiBlue": "#6EAADB",
    "terminal.ansiMagenta": "#A454CB",
    "terminal.ansiCyan": "#4AB5B4",
    "terminal.ansiWhite": "#2D2D2D",
    "terminal.ansiBrightBlack": "#808890",
    "terminal.ansiBrightRed": "#F19196",
    "terminal.ansiBrightGreen": "#8FD0A0",
    "terminal.ansiBrightYellow": "#DCBFA8",
    "terminal.ansiBrightBlue": "#8CA9D5",
    "terminal.ansiBrightMagenta": "#B678D6",
    "terminal.ansiBrightCyan": "#489DAC",
    "terminal.ansiBrightWhite": "#000000",
    "textLink.foreground": "#3574A3",
    "textLink.activeForeground": "#4B5EBD"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#888888", "fontStyle": "italic" }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string"],
      "settings": { "foreground": "#2F7C61" }
    },
    {
      "name": "Numbers / Constants",
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": { "foreground": "#D4AB74" }
    },
    {
      "name": "Keywords & Storage",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "punctuation.definition.keyword"
      ],
      "settings": { "foreground": "#A454CB" }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": { "foreground": "#4B5EBD" }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "meta.definition.variable.name",
        "support.variable"
      ],
      "settings": { "foreground": "#4AB5B4" }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter", "meta.function.parameters variable"],
      "settings": { "foreground": "#489DAC" }
    },
    {
      "name": "HTML Tags",
      "scope": [
        "entity.name.tag",
        "entity.name.tag.jsx",
        "entity.name.tag.tsx"
      ],
      "settings": { "foreground": "#4B5EBD", "fontStyle": "bold" }
    },
    {
      "name": "Angle Brackets",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.jsx",
        "punctuation.definition.tag.tsx"
      ],
      "settings": { "foreground": "#999999" }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name.vue",
        "meta.directive.vue"
      ],
      "settings": { "foreground": "#3574A3" }
    },
    {
      "name": "Attribute Values (HTML/JSX/Vue)",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.quoted.double.jsx",
        "string.quoted.single.jsx",
        "string.quoted.double.tsx",
        "string.quoted.single.tsx",
        "string.quoted.double.vue",
        "string.quoted.single.vue"
      ],
      "settings": { "foreground": "#2F7C61" }
    },
    {
      "name": "Tailwind Utility Classes",
      "scope": [
        "string.quoted.double.html class",
        "string.quoted.single.html class",
        "meta.attribute.class.html string.quoted",
        "meta.attribute.class.vue string.quoted"
      ],
      "settings": { "foreground": "#31836A" }
    }
  ]
}
